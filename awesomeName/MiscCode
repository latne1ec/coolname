///SOURCE & TIME AGO
self.sourceTimeAgoLabel = [[UILabel alloc] initWithFrame:CGRectMake(18, self.titleLabel.frame.origin.y+66, self.frame.size.width-46, 80)];
if ([UIScreen mainScreen].bounds.size.height <= 568.0 ) {
self.sourceTimeAgoLabel.font = [UIFont fontWithName:@"AppleSDGothicNeo-Medium" size:11];
} else {
self.sourceTimeAgoLabel.font = [UIFont fontWithName:@"AppleSDGothicNeo-Medium" size:13];
}
self.sourceTimeAgoLabel.textColor = [UIColor colorWithWhite:1.0 alpha:0.94];
self.sourceTimeAgoLabel.numberOfLines = 5;
self.sourceTimeAgoLabel.tag = 5555;
self.sourceTimeAgoLabel.layer.shadowColor = [UIColor blackColor].CGColor;
self.sourceTimeAgoLabel.layer.shadowOpacity = 0.12;
self.sourceTimeAgoLabel.layer.shadowRadius = 0.20;
self.sourceTimeAgoLabel.layer.shadowOffset = CGSizeMake(.20f, .20f);
[self.scrollView addSubview:self.sourceTimeAgoLabel];
[self.scrollView bringSubviewToFront:self.sourceTimeAgoLabel];




